{
  "schemaVersion": 1,
  "id": "ifsv",
  "version": "${version}",
  "name": "itemframe shulker viewer addon",
  "description": "an addon which let's you see whats inside a shulker box without taking them out of the itemframe.",
  "authors": [
    "HoseanRC"
  ],
  "contact": {
    "repo": "https://github.com/HoseanRC/IFSV"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "org.hoseanrc.ifsv.Addon"
    ]
  },
  "mixins": [
    "addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
